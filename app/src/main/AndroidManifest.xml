<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bruker.hangman">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Monty"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".MainWindow.Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Statistics.Statistics"
            android:parentActivityName=".MainWindow.Main" />
        <activity android:name=".Settings.Settings"
            android:parentActivityName=".MainWindow.Main"></activity>
    </application>

</manifest>